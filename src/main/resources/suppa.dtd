<!ELEMENT supplemental-pub-data (supp.title-block, process-category?, supp.data-block)>
<!ATTLIST supplemental-pub-data
	id ID #REQUIRED
	label CDATA #FIXED "[Supplemental Publication Data:] "
	class CDATA #FIXED "legal-instrument"
>
<!ELEMENT supp.title-block (supp.title, supp.version)>
<!ATTLIST supp.title-block
	label CDATA #FIXED "[Title Block:] "
	class CDATA #FIXED "template-id"
>
<!ELEMENT supp.title EMPTY>
<!ATTLIST supp.title
	label CDATA #FIXED "[Title:] "
	class CDATA #FIXED "title"
	text CDATA #FIXED "SUPPLEMENTAL PUBLICATION DATA"
>
<!ELEMENT supp.version EMPTY>
<!ATTLIST supp.version
	class CDATA #FIXED "version"
	distribution CDATA #FIXED "2012-12-04"
	label CDATA #FIXED "[Version:] "
	version CDATA #FIXED "[Electronic Version 0.0.8] "
>
<!ELEMENT process-category (classification-outsourcing | edc | pgpub | grant)>
<!ELEMENT classification-outsourcing (normal | special | research | pgpub-classification-correction | grant-classification-correction)>
<!ELEMENT normal EMPTY>
<!ELEMENT special EMPTY>
<!ELEMENT research EMPTY>
<!ELEMENT pgpub-classification-correction EMPTY>
<!ELEMENT grant-classification-correction EMPTY>
<!ELEMENT edc (initial | final)>
<!ELEMENT initial EMPTY>
<!ELEMENT final EMPTY>
<!ELEMENT pgpub (initial | final)>
<!ELEMENT grant (initial | final)>
<!ELEMENT supp.data-block (application-number, uspto-org-unit-name?, application-category?, invention-subject-matter?, notice-of-allowance-date?, domestic-filing-data?, projected-publication-date?, representative-figure?, classifications-cpc?, classification-us?, classifications-ipcr?, prior-publication?, international-conventions?, sequence-listing-available?)>
<!ATTLIST supp.data-block
	label CDATA #FIXED "[Supplemental Data Block:] "
>
<!ELEMENT application-number (#PCDATA)>
<!ATTLIST application-number
	label CDATA #FIXED "[Application No:] "
	class CDATA #FIXED "primitive"
	data-format CDATA #FIXED "##/###,###|########|##/######"
>
<!ELEMENT uspto-org-unit-name (#PCDATA)>
<!ELEMENT application-category (regular | provisional | reissue | reexam)>
<!ELEMENT regular EMPTY>
<!ELEMENT provisional EMPTY>
<!ELEMENT reissue EMPTY>
<!ELEMENT reexam EMPTY>
<!ELEMENT invention-subject-matter (plant | utility | design)>
<!ELEMENT plant EMPTY>
<!ELEMENT utility EMPTY>
<!ELEMENT design EMPTY>
<!ELEMENT notice-of-allowance-date (#PCDATA)>
<!ELEMENT domestic-filing-data (application-number, application-number-series-code, filing-date, continued-prosecution-application-flag?, rule-47-flag?)>
<!ATTLIST domestic-filing-data
	label CDATA #FIXED "[Domestic Filing Data:] "
>
<!ELEMENT application-number-series-code (#PCDATA)>
<!ATTLIST application-number-series-code
	label CDATA #FIXED "[Application Number Series Code:] "
>
<!ELEMENT filing-date (#PCDATA)>
<!ATTLIST filing-date
	label CDATA #FIXED "[Filing Date:] "
>
<!ELEMENT continued-prosecution-application-flag EMPTY>
<!ATTLIST continued-prosecution-application-flag
	label CDATA #FIXED "[Continued Prosecution Application Flag:] "
>
<!ELEMENT rule-47-flag EMPTY>
<!ATTLIST rule-47-flag
	label CDATA #FIXED "[Rule 47 Flag:] "
>
<!ELEMENT projected-publication-date (#PCDATA)>
<!ATTLIST projected-publication-date
	label CDATA #FIXED "[Projected Publication Date:] "
>
<!ELEMENT representative-figure (#PCDATA)>
<!ATTLIST representative-figure
	label CDATA #FIXED "[Representative Figure:] "
>
<!ELEMENT classification-us (classification-us-primary, classification-us-secondary*)>
<!ATTLIST classification-us
	label CDATA #FIXED "[US Classification:] "
	class CDATA #FIXED "classification"
>
<!ELEMENT classification-us-primary (uspc)>
<!ATTLIST classification-us-primary
	label CDATA #FIXED "[US Classification (Primary):] "
	class CDATA #FIXED "classification"
>
<!ELEMENT uspc (class, subclass, classification-value?)>
<!ATTLIST uspc
	label CDATA #FIXED "[US Patent Classification:] "
>
<!ELEMENT class (#PCDATA)>
<!ATTLIST class
	label CDATA #FIXED "[Class:] "
>
<!ELEMENT subclass (#PCDATA)>
<!ATTLIST subclass
	label CDATA #FIXED "[Subclass:] "
>
<!ELEMENT classification-us-secondary (uspc | uspc-freeform)>
<!ATTLIST classification-us-secondary
	label CDATA #FIXED "[US Classification (Secondary):] "
	class CDATA #FIXED "classification"
>
<!ELEMENT uspc-freeform (#PCDATA)>
<!ATTLIST uspc-freeform
	label CDATA #FIXED "[US Patent Classification, freeform:] "
	class CDATA #FIXED "primitiv"
>
<!ELEMENT classifications-ipcr (classification-ipcr+)>
<!ATTLIST classifications-ipcr
	id ID #IMPLIED
>
<!ELEMENT classification-ipcr (ipc-version-indicator, classification-level?, section, class, subclass, main-group?, subgroup?, symbol-position?, classification-value?, action-date?, generating-office?, classification-status?, classification-data-source?)>
<!ATTLIST classification-ipcr
	id ID #IMPLIED
	sequence CDATA #IMPLIED
>
<!-- -->
<!ELEMENT main-cpc (classification-cpc)>
<!ATTLIST main-cpc
	id ID #IMPLIED
>
<!-- -->
<!ELEMENT further-cpc (classification-cpc*, combination-set*)>
<!ATTLIST further-cpc
	id ID #IMPLIED
>
<!-- -->
<!ELEMENT classifications-cpc (main-cpc, further-cpc?)>
<!ATTLIST classifications-cpc
	id ID #IMPLIED
>
<!-- Cooperative Patent Classification scheme -->
<!ELEMENT classification-cpc (cpc-version-indicator, section, class, subclass, main-group, subgroup, symbol-position, classification-value)>
<!ATTLIST classification-cpc
	id ID #IMPLIED
	sequence CDATA #IMPLIED
>
<!-- The publication date of the CPC group assigned. -->     
<!ELEMENT cpc-version-indicator (date)>
<!-- A combination set is a group of CPC symbols that have one base class and one or more subsequent ranked symbols that are linked together to convey special classification information.-->
<!ELEMENT combination-set (group-number, combination-rank+)>
<!ATTLIST combination-set
	id ID #IMPLIED
>
<!-- -->
<!ELEMENT combination-rank (rank-number, classification-cpc)>
<!ATTLIST combination-rank
	id ID #IMPLIED
>
<!-- The sequential number that is used to identify the rank of a symbol within a combination set.  (Order of the symbols is important). -->
<!ELEMENT rank-number (#PCDATA)>
<!-- A number that is used to identify a group of symbols, when allocating a combination set of symbols to a patent document.-->
<!ELEMENT group-number (#PCDATA)>
<!-- -->
<!ELEMENT ipc-version-indicator (date)>
<!-- -->
<!ELEMENT date (#PCDATA)>
<!-- The code that classifies the subject matter level.   C (Core), A (Advanced) , S (Subclass), blank (for CPC). -->
<!ELEMENT classification-level (#PCDATA)>
<!--  The highest hierarchical level within the classification scheme and as such it represents the whole body of knowledge which may be regarded as proper to the field of Classification. -->
<!ELEMENT section (#PCDATA)>
<!-- The code denotes the fourth level subdivision of the classification scheme and as such is a further breakdown of the novel subject matter. -->
<!ELEMENT main-group (#PCDATA)>
<!-- The code denotes the fifth level subdivision of the classification scheme and as such is a further breakdown of the novel subject matter. -->
<!ELEMENT subgroup (#PCDATA)>
<!-- The code that specifies the position of the classification symbol. -->
<!ELEMENT symbol-position (#PCDATA)>
<!-- The code that distinguishes between invention information (invention) and other information (non-invention/additional), when describing a classification symbol on a document. -->
<!ELEMENT classification-value (#PCDATA)>
<!--The date the symbol was allocated to the patent document. -->
<!ELEMENT action-date (date)>
<!--The IP Office that allocated the classification symbol to the patent document.  The code is based on WIPO Standard ST.3. -->
<!ELEMENT generating-office (country)>
<!--Country: use ST.3 country code, e.g. DE, FR, GB, NL, etc. Also includes EP, WO, etc.-->
<!ELEMENT country (#PCDATA)>
<!-- The code that describes the circumstances under which the symbol was allocated to the patent document. -->
<!ELEMENT classification-status (#PCDATA)>
<!-- The code that describes the source of the allocation of the symbol to the patent document. -->
<!ELEMENT classification-data-source (#PCDATA)>
<!-- -->
<!ELEMENT prior-publication (document-id+)>
<!ATTLIST prior-publication
	label CDATA #FIXED "[Prior Publication:] "
>
<!ELEMENT document-id (doc-number, kind-code?, document-date?, country-code?)>
<!ATTLIST document-id
	label CDATA #FIXED "[Document ID:] "
	class CDATA #FIXED "primitiv"
>
<!ELEMENT doc-number (#PCDATA)>
<!ATTLIST doc-number
	label CDATA #FIXED "[Doc Number:] "
	class CDATA #FIXED "primitiv"
>
<!ELEMENT kind-code (#PCDATA)>
<!ATTLIST kind-code
	label CDATA #FIXED "[KIND Code:] "
	class CDATA #FIXED "primitiv"
>
<!ELEMENT document-date (#PCDATA)>
<!ATTLIST document-date
	label CDATA #FIXED "[Document Date:] "
	class CDATA #FIXED "primitive"
>
<!ELEMENT country-code (#PCDATA)>
<!ATTLIST country-code
	label CDATA #FIXED "[Country Code:] "
	class CDATA #FIXED "primitiv"
>
<!ELEMENT international-conventions (pct-application, usc371-date?, usc102e-date, pct-publication?)>
<!ATTLIST international-conventions
	label CDATA #FIXED "[International Conventions:] "
	st32-name CDATA #FIXED "B800"
	inid-code CDATA #IMPLIED
>
<!ELEMENT pct-application (document-id)>
<!ATTLIST pct-application
	label CDATA #FIXED "[PCT Application:] "
	st32-name CDATA #FIXED "B860"
	inid-code CDATA #IMPLIED
>
<!ELEMENT usc371-date (#PCDATA)>
<!ATTLIST usc371-date
	label CDATA #FIXED "[USC371 Date:] "
	st32-name CDATA #FIXED "B863"
	inid-code CDATA #IMPLIED
>
<!ELEMENT usc102e-date (#PCDATA)>
<!ATTLIST usc102e-date
	label CDATA #FIXED "[USC102e Date:] "
	st32-name CDATA #FIXED "B864"
>
<!ELEMENT pct-publication (document-id)>
<!ATTLIST pct-publication
	label CDATA #FIXED "[PCT Publication:] "
	st32-name CDATA #FIXED "B871"
	inid-code CDATA #IMPLIED
>
<!ELEMENT sequence-listing-available (publish-sequence-listing)>
<!ATTLIST sequence-listing-available
	label CDATA #FIXED "[Sequence Listing Available:] "
>
<!ELEMENT publish-sequence-listing EMPTY>
<!ATTLIST publish-sequence-listing
	label CDATA #FIXED "[Publish Sequence Listing:] "
	value (yes | no) "yes"
>
